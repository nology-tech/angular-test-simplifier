{
  "name": "@nology/angular-test-simplifier",
  "version": "0.1.5",
  "description": "Simplifies testing in angular/cli applications",
  "main": "dist/index.js",
  "scripts": {
    "test": "npm run build && jasmine",
    "build": "gulp clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "pub": "npm run build && npm publish"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nology-tech/angular-test-simplifier.git"
  },
  "author": "Nology",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nology-tech/angular-test-simplifier/issues"
  },
  "homepage": "https://github.com/nology-tech/angular-test-simplifier#readme",
  "devDependencies": {
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@types/jasmine": "^3.4.0",
    "@types/node": "^12.7.2",
    "codelyzer": "^5.1.0",
    "del": "^5.0.0",
    "gulp": "^4.0.2",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "mem": ">=4.0.0",
    "prettier": "^1.18.2",
    "rxjs": "^6.5.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3",
    "zone.js": "^0.10.2"
  },
  "peerDependencies": {
    "@angular/core": "^8.x",
    "@angular/compiler": "^8.x",
    "rxjs": "^6.x"
  },
  "dependencies": {
    "@angular/common": "^8.2.2",
    "@angular/platform-browser": "^8.2.2"
  }
}
